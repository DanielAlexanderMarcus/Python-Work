import csv
import os
import pyodbc

conn = pyodbc.connect('Driver={SQL Server};'
                      'Server=DESKTOP-D9AJQ7F\SQLEXPRESS;'
                      'Database=LOCAL;'
                      'Trusted_Connection=yes;')


files = os.listdir('C:\\')
for i in files:
    if i.endswith('.csv')==1:
        with open('C:\\'+i) as file:
            my_reader = csv.reader(file, delimiter=',')
            print(i)
            for row in my_reader:
               print(row)



cursor = conn.cursor()
cursor.execute('SELECT * FROM dbo.dim_brands')

for row in cursor:
    print(row)
